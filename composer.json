    "require": {
        "php": "^7.4 || ^8.0",
        "sylius/plugin-skeleton": "^1.10.x-dev"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.4 || ^5.2",
        "symfony/debug-bundle": "^4.4 || ^5.2",
        "symfony/dotenv": "^4.4 || ^5.2",
        "symfony/intl": "^4.4 || ^5.2",
        "symfony/web-profiler-bundle": "^4.4 || ^5.2",
        "sylius-labs/coding-standard": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "PiotrKardasz\\SyliusPluginSkeleton\\": "src/",
            "Tests\\PiotrKardasz\\SyliusPluginSkeleton\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
